{
  "name": "trading-vue-js",
  "version": "0.5.0",
  "description": "Hackable charting lib for traders",
  "main": "./dist/trading-vue.js",
  "scripts": {
    "test": "webpack-dev-server --config webpack/test.config.js --mode development --progress --hot",
    "auto-test": "ava",
    "eslint": "./node_modules/.bin/eslint --ext .js,.vue src",
    "dev": "webpack-dev-server --config webpack/dev.config.js --mode development --progress --hot",
    "build": "webpack  --config webpack/build.config.js --mode production --progress"
  },
  "ava": {
    "babel": {
        "compileAsTests": [
            "test/auto_tests/**/*",
            "src/**/*"
        ]
    },
    "files": [
      "test/auto_tests/**/*"
    ]
  },
  "author": "C451",
  "license": "MIT",
  "files": [
    "dist/*",
    "src/*"
  ],
  "keywords": [
    "trading",
    "vue",
    "charts",
    "crypto",
    "visualization",
    "reactive"
  ],
  "homepage": "https://github.com/C451/trading-vue-js#readme",
  "repository": "github:C451/trading-vue-js",
  "devDependencies": {
    "@ava/babel": "^1.0.1",
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "ava": "^3.7.0",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.5.2",
    "eslint": "^6.8.0",
    "eslint-config-eslint": "^6.0.0",
    "eslint-plugin-vue": "^6.2.2",
    "html-webpack-plugin": "^4.2.0",
    "request": "^2.88.2",
    "terser-webpack-plugin": "^2.3.5",
    "tulind": "^0.8.18",
    "vue-loader": "^15.9.1",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "@babel/runtime": "^7.9.2",
    "arrayslicer": "^1.2.3",
    "hammerjs": "^2.0.8",
    "hamsterjs": "^1.1.3",
    "moment": "^2.24.0",
    "vue": "^2.6.11"
  }
}
